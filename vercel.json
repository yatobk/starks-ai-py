{
    "version": 2,
    "builds": [
        {
            "src": "src/app/index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/api/routes.py"
        }
    ]
}